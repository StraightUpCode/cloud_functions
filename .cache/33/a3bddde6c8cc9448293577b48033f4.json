erated":{"line":108,"column":0},"source":"js/Components/Home/Login/CustomAuthButton.js","original":{"line":51,"column":3}},{"name":"Component","generated":{"line":111,"column":2},"source":"js/Components/Home/Login/CustomAuthButton.js","original":{"line":4,"column":31}},{"name":"CustomAuthButton","generated":{"line":113,"column":15},"source":"js/Components/Home/Login/CustomAuthButton.js","original":{"line":54,"column":15}}],"sources":{"js/Components/Home/Login/CustomAuthButton.js":"import { h, Component } from 'preact'\nimport Portal from 'preact-portal'\n\nclass CustomAuthButton extends Component {\n  state = {\n    email: '',\n    password: '',\n    open: false\n  }\n  togglePortal = e => {\n    e.stopPropagation()\n    this.setState((prevState) => ({ open: !prevState.open }))\n  }\n\n  handleChange = e => {\n    let name = e.target.name\n    let value = e.target.value\n    this.setState({ [name]: value })\n  }\n  handleSubmit = e => {\n    e.preventDefault()\n    let { email, password } = this.state\n    this.props.handleSubmit(email, password)\n  }\n\n  render({ errorMsg, type }, { open }) {\n    return (\n      <div >\n        <button onClick={this.togglePortal} >{type}</button>\n        {open ?\n          <Portal into=\"body\">\n            <div class=\"portal\" >\n              <div class=\"portal-content\">\n                <div onClick={this.togglePortal} >Close</div>\n                <form onSubmit={this.handleSubmit}\n                >\n                  <input name=\"email\" type=\"text\" placeholder=\"example@email.com\" onChange={this.handleChange} />\n\n                  <input name=\"password\" type=\"password\" placeholder=\"******\" onChange={this.handleChange} />\n                  <button class=\"button\" type=\"submit\">Submit</button>\n                </form>\n\n              </div>\n            </div>\n          </Portal>\n          : null}\n        {errorMsg ? <span>Error {errorMsg}</span> : null}\n      </div >\n\n    )\n  }\n}\n\nexport default CustomAuthButton "},"lineCount":null}},"hash":"e96c687622d067c7d52dc9e46c235428","cacheData":{"env":{}}}