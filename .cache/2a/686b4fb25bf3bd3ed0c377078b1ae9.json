generated":{"line":112,"column":8},"source":"js/Components/Home/Login/Login.js","original":{"line":58,"column":6}},{"generated":{"line":112,"column":10},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":8}},{"generated":{"line":113,"column":0},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":16}},{"generated":{"line":113,"column":8},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":16}},{"generated":{"line":113,"column":15},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":23}},{"generated":{"line":113,"column":17},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":25}},{"name":"signInGoogle","generated":{"line":113,"column":22},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":30}},{"generated":{"line":114,"column":0},"source":"js/Components/Home/Login/Login.js","original":{"line":62,"column":8}},{"generated":{"line":114,"column":27},"source":"js/Components/Home/Login/Login.js","original":{"line":58,"column":6}},{"generated":{"line":114,"column":28},"source":"js/Components/Home/Login/Login.js","original":{"line":56,"column":4}},{"generated":{"line":115,"column":0},"source":"js/Components/Home/Login/Login.js","original":{"line":64,"column":3}},{"name":"Component","generated":{"line":118,"column":2},"source":"js/Components/Home/Login/Login.js","original":{"line":9,"column":20}},{"name":"Login","generated":{"line":120,"column":15},"source":"js/Components/Home/Login/Login.js","original":{"line":67,"column":15}}],"sources":{"js/Components/Home/Login/Login.js":"import { h, Component } from 'preact'\nimport { route } from 'preact-router'\nimport { firebase, FirebaseAuth, Firestore } from '../../../firebase'\nimport 'firebase/auth'\nimport CustomAuthButton from './CustomAuthButton'\nimport RegisterButton from './RegisterButton'\nimport './Login.scss'\n\nclass Login extends Component {\n\n  state = {\n    provider: new firebase.auth.GoogleAuthProvider()\n  }\n\n  handleLogin = (e) => { }\n\n  signInGoogle = e => {\n    FirebaseAuth.signInWithPopup(this.state.provider)\n      .then(result => {\n        route('/')\n      })\n\n  }\n  registerUser = (email, password, nickname) => {\n    FirebaseAuth.createUserWithEmailAndPassword(email, password)\n      .then(result => {\n        console.log(result)\n        return result.user.uid\n      })\n      .then(userId => {\n        console.log(userId)\n        Firestore.collection(\"users\")\n          .doc(userId)\n          .set({ nickname })\n      })\n      .then(_ => route('/'))\n      .catch(err =>\n        this.setState({ registerError: err.message })\n      )\n  }\n  signIn = (email, password) => {\n    FirebaseAuth.signInWithEmailAndPassword(email, password)\n      .then(result => {\n        console.log(result)\n        return result.user.uid\n      })\n      .then(userId => {\n        console.log(userId)\n      })\n      .then(_ => route('/'))\n      .catch(err => {\n        this.setState({ signInError: err.message })\n      })\n  }\n  render(props, { registerError, signInError }) {\n    return (\n\n      <div>\n        Log in page\n        <CustomAuthButton handleSubmit={this.signIn} type=\"Log In\" errorMsg={signInError} />\n        <RegisterButton handleSubmit={this.registerUser} type=\"Register\" errorMsg={registerError} />\n        <button onClick={this.signInGoogle}>Sign In Google </button>\n      </div>)\n  }\n}\n\nexport default Login"},"lineCount":null}},"hash":"ee830732fe738f84a287796249097ed4","cacheData":{"env":{}}}